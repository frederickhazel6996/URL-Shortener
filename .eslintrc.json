{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-undef": "warn",
        "no-unused-vars": "warn",
        "no-empty": "off"
    },
    "globals": {
        "dbService": false,
        "_validator": false
    }
}
